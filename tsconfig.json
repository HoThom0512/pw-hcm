{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@fixtures/*": ["src/fixture/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": [
  "features/**/*.ts",
  "step_definitions/**/*.ts",
  "*.ts"
]
}
